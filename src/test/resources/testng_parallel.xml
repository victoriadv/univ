<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ParallelSuite" parallel="tests" thread-count="2">
    <test name="GroupATests">
        <groups>
            <run>
                <include name="GroupA"/>
            </run>
        </groups>
        <classes>
            <class name="com.beginsecure.domain.DataSorterTest"/>
            <class name="com.beginsecure.domain.TextParserTest"/>
        </classes>
    </test>
    <test name="GroupBTests">
        <groups>
            <run>
                <exclude name="GroupA"/>
                <include name="GroupB"/>
            </run>
        </groups>
        <classes>
            <class name="com.beginsecure.domain.DataSorterTest"/>
            <class name="com.beginsecure.domain.TextParserTest"/>
        </classes>
    </test>
</suite>
